{
  "llvm-target": "avr-atmega328p",
  "target-endian": "little",
  "target-pointer-width": "16",
  "target-c-int-width": "16",
  "data-layout": "e-m:o-p:16:16-i8:8:8-i16:16:16-n8:16:32",
  "arch": "avr",
  "os": "none",
  "executables": true,
  "linker-flavor": "gcc",
  "linker": "avr-gcc",
  "pre-link-args": {
    "gcc": ["-mmcu=atmega328p"]
  },
  "post-link-args": {
    "gcc": ["-Wl,--gc-sections"]
  },
  "max-atomic-width": 8
}
